#include <stdio.h>
#include <stdlib.h>

int main()
{

    int n;

    printf("Digite um numero: ");
    scanf("%i", &n);


    for(int i = 1; i <= n; i++){
        for(int f = 1; f <= n; f++){
            if(i == f){
                printf(" 1 ");
            }else{
                printf(" 0 ");
            }
        }
        printf("\n");
    }

    return 0;
}
