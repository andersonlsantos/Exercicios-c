#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n, cont = 0, t1, t2, t3, t4, t5;

    for(int turma = 1; turma <= 5; turma++){
        printf("turma %i\n", turma);
        for(int alunos = 1; alunos <= 5; alunos++){
            printf("Aluno numero %i digite sua nota: ", alunos);
            scanf("%i", &n);
            getchar();
            cont += n;
        }
        if(turma == 1){
            t1 = cont;
        }
        if(turma == 2){
            t2 = cont - t1;
        }
        if(turma == 3){
            t3 = cont - t2 - t1;
        }
        if(turma == 4){
            t4 = cont - t3 - t2 - t1;
        }
        if(turma == 5){
            t5 = cont - t4 - t3 - t2 - t1;
        }
        printf("\n");
    }

    printf("Média da turma 1 = %i\n", t1 / 5);
    printf("Média da turma 2 = %i\n", t2 / 5);
    printf("Média da turma 3 = %i\n", t3 / 5);
    printf("Média da turma 4 = %i\n", t4 / 5);
    printf("Média da turma 5 = %i\n", t5 / 5);

    return 0;
}
