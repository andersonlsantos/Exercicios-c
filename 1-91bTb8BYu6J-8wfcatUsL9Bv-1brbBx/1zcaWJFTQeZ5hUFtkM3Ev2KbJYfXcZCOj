#include <stdio.h>
#include <stdlib.h>

int main()
{

    int numero, cont = 0;

    printf("Digite um numero: ");
    scanf("%i", &numero);

    for(int i = 1;i <= numero;i++){
        if(numero % i == 0){
            printf("%i\n", i);
            cont += i;
        }

    }

    printf("A soma dos divisores do número %i é %i", numero, cont - numero);

    return 0;
}
