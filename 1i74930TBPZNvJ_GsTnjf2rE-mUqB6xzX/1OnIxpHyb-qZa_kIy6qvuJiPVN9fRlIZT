#include <stdio.h>
#include <stdlib.h>

int main()
{

    int numero, cont = 0;

    printf("Digite um numero: ");
    scanf("%i", &numero);

    for(int i = 1;i <= numero;i++){
        if(numero % i == 0){
            printf("%i\n", i);
            cont += 1;
        }
    }

    printf("----------------------------------\n");
    if(cont == 2){
        printf("%i é primo!", numero);
    }else{
        printf("%i não é primo!", numero);
    }

    return 0;
}
